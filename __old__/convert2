=IFERROR(VALUE(TRIM(MID(SUBSTITUTE(A1,"s",REPT(" ",99)),MAX(1,FIND("s",A1)-98),99))),0)
+ IFERROR(VALUE(TRIM(MID(SUBSTITUTE(A1,"m",REPT(" ",99)),MAX(1,FIND("m",A1)-98),99)))*60,0)
+ IFERROR(VALUE(TRIM(MID(SUBSTITUTE(A1,"h",REPT(" ",99)),MAX(1,FIND("h",A1)-98),99)))*3600,0)
+ IFERROR(VALUE(TRIM(MID(SUBSTITUTE(A1,"d",REPT(" ",99)),MAX(1,FIND("d",A1)-98),99)))*86400,0)
+ IFERROR(VALUE(TRIM(MID(SUBSTITUTE(A1,"w",REPT(" ",99)),MAX(1,FIND("w",A1)-98),99)))*604800,0)
